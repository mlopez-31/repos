<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="ensamblados" />
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="empresa" value="1" />
    <add key="oficina" value="948" />
    <add key="CommandTimeout" value="50" />
    <add key="maxFilas" value="100" />
    <add key="SqlLocal" value="." />
    <add key="SqlCentral" value="." />
    <add key="debug" value="False" />
    <add key="usuario" value="opnralon" />
    <add key="DirLog" value="C:\Syscards\logs" />
    <add key="rutaProgramas" value="C:\Syscards\Win\clientasm" />
    <add key="VersionServer" value="True" />
    <add key="VersionInicio" value="True" />
    <add key="VersionCadaPeticion" value="True" />
    <add key="ModoConexionActiveDirectory" value="true" />
    <add key="ActiveDirectoryPortSecure" value="389" />
    <add key="ActiveDirectoryServer" value="Techsoft.Oficina" />
    <add key="ActiveDirectoryLDAP" value="CN=Users,DC=TechSoft,DC=Oficina" />
    <add key="trace" value="true" />
  </appSettings>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_IObjectBusiness" maxBufferSize="5000000"
          maxReceivedMessageSize="5000000">
          <readerQuotas maxDepth="32" maxStringContentLength="5000000"
            maxArrayLength="5000000" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <security>
            <transport clientCredentialType="Certificate" />
          </security>
        </binding>
        <binding name="NetTcpBinding_IVersionServer" maxBufferSize="5000000"
          maxReceivedMessageSize="5000000">
          <readerQuotas maxDepth="32" maxStringContentLength="5000000"
            maxArrayLength="5000000" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <security mode="None" />
        </binding>
        <binding name="NetTcpBinding_Batch" maxBufferSize="5000000" maxReceivedMessageSize="5000000">
          <readerQuotas maxDepth="32" maxStringContentLength="5000000"
            maxArrayLength="5000000" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <security>
            <transport clientCredentialType="Certificate" />
          </security>
        </binding>
      </netTcpBinding>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IServicioWCF">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384"
            maxBytesPerRead="4096" maxNameTableCharCount="16384" />
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://192.168.1.79:1106/netTcpBinding/TransacServer.ServiciosWCF/ObjectBusiness/"
        behaviorConfiguration="clientEndpointCredential" binding="netTcpBinding"
        bindingConfiguration="NetTcpBinding_IObjectBusiness" contract="TransacServer.ObjectBusiness.IObjectBusiness"
        name="NetTcpBinding_IObjectBusiness">
        <identity>
          <dns value="TechSoft CA2" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://192.168.1.79:5001/netTcpBinding/ts.VersionServer/VersionServer/"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IVersionServer"
        contract="VersionServer.IVersionServer" name="NetTcpBinding_IVersionServer">
        <identity>
          <dns value="192.168.1.79" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://192.168.1.79:6551/Design_Time_Addresses/BatchServer/ServicioWCF/"
        behaviorConfiguration="clientEndpointCredential" binding="netTcpBinding"
        bindingConfiguration="NetTcpBinding_Batch" contract="svcBatchServer.IServicioWCF"
        name="WSHttpBinding_IServicioWCF">
        <identity>
          <dns value="TechSoft CA2" />
        </identity>
      </endpoint>
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="clientEndpointCredential">
          <clientCredentials>
            <clientCertificate findValue="TechSoft CA2" x509FindType="FindBySubjectName" />
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
  <system.web>
    <trust level="Full" />
    <webControls clientScriptsLocation="/aspnet_client/{0}/{1}/" />
  </system.web>
</configuration>